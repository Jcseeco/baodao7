  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.2",
    "purgecss-webpack-plugin": "^5.0.0",
    "tailwindcss": "^1.4.0",
    "webpack": "^5.75.0"
  }
